<?xml version="1.0" encoding="utf-8"?>
<resources xmlns:tools="http://schemas.android.com/tools"
           tools:keep="@string/cfg_*,@integer/cfg_*">


    <!--********************广告SDK—start********************-->
    <!-- 广告13接口产品配置:产品ID、数据渠道
  配置请找[服务器组*黄炯]
  -->
    <integer name="dy_product_id">@integer/diff_config_basic_services_id</integer>
    <string name="cfg_commerce_cid">@string/diff_config_ab_test_cid</string>
    <string name="cfg_commerce_data_channel">@string/diff_config_ad_background_data_channel</string>
    <!--渠道号 -->
    <string name="cfg_commerce_channel">200</string>

    <!--入口ID, a.主包(默认):1 b.主题:2 -->
    <string name="cfg_commerce_entrance_id">1</string>

    <!--离线广告请求接口产品配置:product_key、access_key
          配置请找[服务器组*黄炯]
           -->
    <string name="cfg_commerce_ab_protocol">@string/diff_config_ab_test_host</string>
    <string name="cfg_commerce_ad_request_product_key">@string/diff_config_ab_test_prodkey</string>
    <string name="cfg_commerce_ad_request_access_key">@string/diff_config_ab_test_accessKey</string>


    <!--105统计产品ID，主包和主题是分开的
    配置请找[大数据组*刘晓宏]
    -->
    <integer name="cfg_commerce_statistic_id_105">@integer/diff_config_statistic_19_product_id</integer>

    <!--智能预加载配置, 可选, 一般只配置3个:智能预加载广告位、native抓取广告位、系统安装抓取广告位, 由梁天丞决定
        配置请找[商业组*梁天丞]
        -->
    <!--[智能预加载]智能预加载广告位，可缺省，或者(默认:-1)-->
    <integer name="cfg_commerce_intelligent_adpos">-1</integer>
    <!--[智能预加载]GP打开Mobvista广告位，可缺省，或者(默认:-1)-->
    <integer name="cfg_commerce_intelligent_adpos_mob">-1</integer>
    <!--[智能预加载]：新用户Mobvista广告位，可缺省，或者(默认:-1)-->
    <integer name="cfg_commerce_intelligent_adpos_mob_new">-1</integer>
    <!--[智能预加载]：安装预点击广告位，可缺省，或者(默认:-1)-->
    <integer name="cfg_commerce_intelligent_adpos_install_preparse">-1</integer>
    <!--native抓取广告位-->
    <integer name="cfg_commerce_native_presolve_request_id">-1</integer>
    <!--系统安装抓取广告位-->
    <integer name="cfg_commerce_system_install_adpos">-1</integer>
    <!--********************广告SDK—end********************-->

    <!--********************充电锁SDK—start********************-->
    <!--插件下发接口产品ID
    配置请找[服务器组*刘长路]
    -->
    <integer name="cfg_chargelocker_plugin_product_id">@integer/diff_config_basic_services_id</integer>
    <!--********************充电锁SDK—end********************-->

    <!--********************插件库—start********************-->
    <!--是否使用输入法旧的统计sdk（输入法外的其它产品配置false）
    -->
    <bool name="cfg_commerce_gokeyboard_statistics">false</bool>
    <!--********************插件库—end********************-->

    <string name="cfg_commerce_appmonet_id">kecatr</string>


    <!--********************插件库—start********************-->
    <!--是否使用输入法旧的统计sdk（输入法外的其它产品配置false）
    -->
    <bool name="cfg_commerce_keyboard_statistics">false</bool>
    <!--********************插件库—end********************-->

    <!--是否小号包地址-->
    <bool name="cfg_commerce_is_new_url">true</bool>
    <string name="cfg_commerce_custom_domain">@string/diff_config_commerce_custom_domain</string>

    <!--产品域名是海外的，项目使用的是国内的，所以使用其他域名-->
    <string name="cfg_commerce_ad_domain">http://newstore-in.xuntongwuxian.com/</string>

</resources>